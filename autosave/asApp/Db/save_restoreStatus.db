record(bo, "$(P)SR_heartbeat") {
  field(DTYP, "Soft Channel")
  field(ZNAM, "0")
  field(ONAM, "1")
  field(FLNK, "$(P)SR_i_am_alive")
}

record(bo, "$(P)SR_i_am_alive") {
  field(DTYP, "Soft Channel")
  field(DOL, "1")
  field(UDF, "0")
  field(ZNAM, "0")
  field(ONAM, "1")
  field(OUT, "$(P)SR_deadIfZero.VAL PP")
}

record(bo, "$(P)SR_deadIfZero") {
  field(DTYP, "Soft Channel")
  field(ZNAM, "0")
  field(ONAM, "1")
  field(HIGH, "$(DEAD_SECONDS=300)")
}

